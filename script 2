-- UI section
local UI = Window:NewTab("Open/Close UI")
local UISection = UI:NewSection("Open/Close")

UISection:NewKeybind("Open/Close UI", "KeybindInfo", Enum.KeyCode.Backspace, function()
    Library:ToggleUI()
end)
