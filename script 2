local function resurrectAndSpawn(spawnArgs)
    local resurrectionArgs = {
        [1] = {
            ["Tribrid"] = true,
            ["Transition"] = false,
            ["Option"] = "revive",
            ["Approved"] = true,
            ["Resurrect"] = true,
        }
    }
    
    ReplicatedStorage:WaitForChild("Events"):WaitForChild("PlayerDeath"):FireServer(unpack(resurrectionArgs))

    wait(0.5)

    spamSpawnCommand(spawnArgs)
end

