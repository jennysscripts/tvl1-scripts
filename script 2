local ReplicatedStorage = game:GetService("ReplicatedStorage")
local doorName = "GarageDoor1" -- Nome da porta que você deseja bloquear
local doorLocked = false -- Estado inicial do loop

-- Função para bloquear a porta
local function lockDoor()
    local args = {
        [1] = {
            ["Locked"] = true, -- Define a porta como trancada
            ["Target"] = workspace:WaitForChild("Doors"):WaitForChild(doorName):WaitForChild("radius"),
            ["Operation"] = "ManageDoor"
        }
    }
    ReplicatedStorage:WaitForChild("Events"):WaitForChild("DoorInteract"):FireServer(unpack(args))
end

-- Função de loop para bloquear a porta repetidamente
local function startLockLoop()
    while doorLocked do
        lockDoor()  -- Tranca a porta
        wait(0.05)  -- Tempo de espera entre as tentativas de trancar (0,05 segundos)
    end
end

-- Criando um NewToggle para controlar o bloqueio da porta com loop
PlayerToolsSection:NewToggle("Spam Garage Lock Door", "", function(state)
    tap:Play()
    doorLocked = state -- Define o estado do loop

    if doorLocked then
        -- Inicia o loop se o toggle estiver ativado
        startLockLoop()
    end
end)

